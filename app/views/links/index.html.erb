<div class="container">
  <h1>Links Index</h1>
  <div class="row">
    <div class='col-md-4'>

      <%= form_for @link, url: link_create_path do |f| %>
        <%= f.label :url_string %>
        <%= f.text_field :url_string, class: "form-control" %>
        <%= f.label :title %>
        <%= f.text_field :title, class: "form-control" %><br  />
        <%= f.submit "Create Link" %>
      <% end %>
    </div>
  </div><br />
  <div class="row">
    <div class="well" id="linksDiv">
    </div>
  </div>
</div>
