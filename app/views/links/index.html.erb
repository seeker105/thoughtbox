<h1>Links#index</h1>
<p>Find me in app/views/links/index.html.erb</p>


<%= form_for @link, url: link_create_path do |f| %>
  <%= f.label :url_string %>
  <%= f.text_field :url_string, class: "form-control" %>
  <%= f.label :title %>
  <%= f.text_field :title, class: "form-control" %>
  <%= f.submit "Create Link" %>
<% end %>
